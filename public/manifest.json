{
  "manifest_version": 3,
  "name": "Orla",
  "version": "2.0",
  "description": "Orla is an AI sidebar assistant for web pages.",
  "key": "",
  "oauth2": {
    "client_id": "110625604157-dd7t9aa7bh0m4dietf5kn10k1qnrq62j.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "openid"
    ]
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "sidePanel",
    "identity"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "webpilot AI"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  }
}